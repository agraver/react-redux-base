{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"],
      "actions/*": ["src/actions/*"],
      "httpClient/*": ["src/httpClient/*"],
      "components/*": ["src/components/*"],
      "middleware/*": ["src/middleware/*"],
      "constants/*": ["src/constants/*"],
      "cypressConstants": ["../cypress/support/constants"],
      "fixtures": ["../cypress/fixtures"],
      "hooks/*": ["src/hooks/*"],
      "locales/*": ["src/locales/*"],
      "pages/*": ["src/pages/*"],
      "reducers/*": ["src/reducers/*"],
      "reusableTests": ["../cypress/support/reusableTests"],
      "selectors/*": ["src/selectors/*"],
      "services/*": ["src/services/*"],
      "store/*": ["src/store/*"],
      "stubs": ["../cypress/stubs"],
      "styles/*": ["src/styles/*"],
      "utils/*": ["src/utils/*"],
    }
  },
  "exclude": ["node_modules", "dist"]
}
